<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".duyurular.fragment_duyurular">

    <jp.co.recruit_lifestyle.android.widget.WaveSwipeRefreshLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/fragment_duyurular_waveswipe">

        <androidx.recyclerview.widget.RecyclerView
            android:id="@+id/fragment_duyurular_recyclerview"
            android:layout_width="match_parent"
            android:layout_height="match_parent"/>


    </jp.co.recruit_lifestyle.android.widget.WaveSwipeRefreshLayout>


    <com.wang.avi.AVLoadingIndicatorView
        android:id="@+id/fragmentyemekhane_progress_avi"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:indicatorName="PacmanIndicator"
        android:layout_gravity="center"
        app:indicatorColor="@color/colorPrimary"/>

    <TextView
        android:id="@+id/fragment_duyurular_textview"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:gravity="center"
        android:layout_margin="10dp"/>

</FrameLayout>